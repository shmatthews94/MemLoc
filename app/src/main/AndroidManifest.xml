<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.android.gms.location.meets.chane.memloc" >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mappinicon"
        android:label="MemLoc"
        android:theme="@style/AppTheme" >
        <activity
            android:name="meets.chane.memloc.LocMem"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="meets.chane.memloc.primaryScreen"
            android:label="@string/title_activity_primary_screen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="meets.chane.memloc.MemBank"
            android:label="@string/title_activity_mem_bank"
            android:parentActivityName="meets.chane.memloc.LocMem" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="meets.chane.memloc.LocMem" />
        </activity>
    </application>

</manifest>
